{
  "name": "correction-diet-course",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --i18n-file=src/locale/messages.ru.xlf --i18n-format=xlf --i18n-locale=ru",
    "build:prod": "ng build --i18n-file=src/locale/messages.en.xlf --i18n-format=xlf --i18n-locale=en; for lang in ru; do ng build --output-path=dist/correction-diet-course/$lang --aot --base-href /$lang/ --i18n-file=src/locale/messages.$lang.xlf --i18n-format=xlf --i18n-locale=$lang; done",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n": "ng xi18n correction-diet-course --i18n-format xlf --output-path src/locale --i18n-locale en && ng run correction-diet-course:xliffmerge",
    "start-ru": "ng serve --configuration=ru"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/compiler": "~8.2.14",
    "@angular/fire": "^6.0.0-rc.1",
    "@angular/forms": "~8.2.14",
    "@angular/localize": "^9.0.1",
    "@angular/material": "^8.2.3",
    "@angular/material-moment-adapter": "^9.0.0",
    "@angular/router": "~8.2.14",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.6",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "firebase": "^7.8.0",
    "firebaseui": "^4.4.0",
    "hammerjs": "^2.0.8",
    "moment": "^2.24.0",
    "rxjs": "^6.5.4",
    "saturn-datepicker": "^8.0.1",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.900.0-0 || ^0.900.0",
    "@angular-devkit/build-angular": "~15.0.4",
    "@angular/cli": "^9.0.3",
    "@angular/common": "^9.0.1",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/core": "^9.0.1",
    "@angular/language-service": "~8.2.14",
    "@angular/platform-browser": "^9.0.1",
    "@angular/platform-browser-dynamic": "^9.0.1",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.6",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "firebase-mock": "^2.3.2",
    "firebase-tools": "^7.12.1",
    "firestore-jest-mock": "^0.2.1",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
